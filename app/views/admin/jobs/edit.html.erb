<div class="col-md-10 col-centerd">
  <div class="panel panel-info">
    <div class="panel-heading">
<h1> Edit a job </h1>
</div>
<div class="panel-body">
<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "薪資下限" %>
<%= f.input :wage_upper_bound, :label => "薪資上限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
  <%= f.submit "Submit" %>
<% end %>
</div>
</div>
</div>
